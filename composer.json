{
    "name" : "fab2s/souuid",
    "description" : "Simple Ordered Uuid Generator in PHP",
    "type" : "library",
    "authors" : [{
        "name" : "Fabrice de Stefanis"
    }],
    "keywords" : [
        "Simple",
        "Ordered",
        "Uuid",
        "PHP",
        "SoUuid"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=5.6.0",
        "paragonie/random_compat": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "symfony/console": "^3.4",
        "webpatser/laravel-uuid": "^3.0|^2.0",
        "ramsey/uuid": "^3.7"
    },
    "suggest": {
        "ext-gmp": "To use base62 and base36 formats",
        "ext-ctype": "To use base62 and base36 formats"
    },
    "autoload": {
        "classmap": [
            "src"
        ]
    }
}
