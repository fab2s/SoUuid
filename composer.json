{
    "name" : "fab2s/souuid",
    "description" : "Simple Ordered Uuid Generator in PHP",
    "type" : "library",
    "authors" : [{
        "name" : "Fabrice de Stefanis"
    }],
    "keywords" : [
        "Simple",
        "Ordered",
        "Uuid",
        "PHP",
        "SoUuid"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~7.0",
        "symfony/console": ">=3.4",
        "webpatser/laravel-uuid": "^3.0|^4.0",
        "ramsey/uuid": "^3.7|^4.0",
        "phpunit/php-timer": "^2.1"
    },
    "suggest": {
        "ext-gmp": "To use base62 and base36 formats",
        "ext-ctype": "To use base62 and base36 formats"
    },
    "autoload": {
        "classmap": [
            "src"
        ]
    },
    "scripts": {
        "post-update-cmd": [
            "rm -f .*.cache"
        ],
        "post-install-cmd": [
            "rm -f .*.cache"
        ]
    }
}
